{
  "manifest_version": 3,
  "name": "TTD Bypass",
  "version": "1.0.0",
  "description": "Resets Timer to 1 second and solves CAPTCHA images.",
  "author": "leitogs",
  "action": {
    "default_icon": {
      "16": "timer_16.png",
      "32": "timer_32.png",
      "48": "timer_48.png",
      "128": "timer_128.png"
    },
    "default_title": "TTD Bypass"
  },
  "icons": {
    "16": "timer_16.png",
    "32": "timer_32.png",
    "48": "timer_48.png",
    "128": "timer_128.png"
  },
  "content_scripts": [
    {
      "run_at": "document_start",
      "matches": ["https://online.tirupatibalaji.ap.gov.in/*"],
      "js": ["cbl.min.js", "content.js"]
    }
  ],
  "permissions": ["storage", "activeTab", "scripting"],
  "host_permissions": ["https://online.tirupatibalaji.ap.gov.in/*"]
}
